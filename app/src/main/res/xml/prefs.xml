<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">



    <ListPreference
        android:key="@string/pref_language_key"
        android:title="@string/pref_language_title"
        android:summary="%s"
        android:dialogTitle="@string/pref_language_title"
        android:entries="@array/pref_language_entries"
        android:entryValues="@array/pref_language_values"
        android:defaultValue="en-US" />

    <CheckBoxPreference
        android:key="@string/pref_in_adult_key"
        android:title="@string/pref_in_adult_title"
        android:defaultValue="false" />
    <EditTextPreference
        android:key="@string/pref_title_key"
        android:title="@string/pref_year"
        />

</PreferenceScreen>